<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework-message-queue:etc/consumer.xsd">
    <consumer name="demoConsumerQueueOne" queue="demo-queue-1" connection="db" consumerInstance="Magento\Framework\MessageQueue\Consumer" handler="\Magento\MysqlMq\Model\Processor::processMessage"/>
    <consumer name="demoConsumerQueueOneWithException" queue="demo-queue-1" connection="db" consumerInstance="Magento\Framework\MessageQueue\Consumer" handler="\Magento\MysqlMq\Model\Processor::processMessageWithException"/>
    <consumer name="demoConsumerQueueTwo" queue="demo-queue-2" connection="db" consumerInstance="Magento\Framework\MessageQueue\Consumer" handler="\Magento\MysqlMq\Model\Processor::processMessage"/>
    <consumer name="demoConsumerQueueThree" queue="demo-queue-3" connection="db" consumerInstance="Magento\Framework\MessageQueue\Consumer" handler="\Magento\MysqlMq\Model\Processor::processMessage"/>
    <consumer name="demoConsumerQueueFour" queue="demo-queue-4" connection="db" consumerInstance="Magento\Framework\MessageQueue\Consumer" handler="\Magento\MysqlMq\Model\Processor::processMessage"/>
    <consumer name="demoConsumerQueueFive" queue="demo-queue-5" connection="db" consumerInstance="Magento\Framework\MessageQueue\Consumer" handler="\Magento\MysqlMq\Model\Processor::processMessage"/>

